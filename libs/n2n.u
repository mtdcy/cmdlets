#!/bin/bash
# Peer-to-peer VPN

# shellcheck disable=SC2034
upkg_name=n2n
upkg_ver=3.1.1
upkg_rev=0
upkg_url=https://github.com/ntop/n2n/archive/refs/tags/$upkg_ver.zip
upkg_zip=n2n-$upkg_ver.zip
upkg_sha=3e3eb29ec3f6c8d79538776baea0eeb2d5f5f041b9c544091b7bee2a3afe44a8
upkg_dep=()

upkg_static() {
    configure &&

    make &&

    cmdlet supernode &&

    cmdlet edge &&

    check supernode -h
}

