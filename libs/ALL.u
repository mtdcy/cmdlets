# all cmdlets

# shellcheck disable=SC2034
upkg_type=PHONY

upkg_ver="$(cat VERSION)"
upkg_rev=0

# only cmdlets here, no libraries
upkg_dep=(
    # zip
    gtar unrar unzip
    # utils
    coreutils gsed gawk findutils grep gmake
    # shell
    bash ctags shfmt
    # editor
    neovim
    # net
    wget curl iperf3
    # multimedia
    ffmpeg
    # misc
    neofetch lazygit act
)

