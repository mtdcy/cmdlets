# all cmdlets

# shellcheck disable=SC2034
upkg_type=PHONY

upkg_ver=0.1.0

upkg_dep=(
    # zip
    gtar unrar unzip
    # utils
    coreutils gsed gawk gfind grep gmake
    # shell
    bash
    # editor
    neovim
    # net
    wget iperf3
    # multimedia
    ffmpeg ffmpeg@4 mac
    # misc
    neofetch
)

# not work with macOS
is_darwin || upkg_dep+=(
    bash@3.2
    bash@4.4
) || true

